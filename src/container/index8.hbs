<main class='p-4'>
  <section>
    <div class='mt-5'>
      <h4 class='mb-4'>{{main.title}}</h4>
      <p>{{main.definition}}</p>
    </div>
  </section>

  <section>
    <div class='row g-2'>
      {{#each pages}}
        <div class='col-3'><a
            class='btn btn-primary py-0 px-5 text-capitalize'
            href='{{href}}'
          >{{name}}</a></div>
      {{/each}}
    </div>
  </section>
</main>